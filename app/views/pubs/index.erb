<% for pub in @pubs %>
<%= pub.id %>:
  id: <%= pub.id %>
  date_sequence: <%= pub.date_sequence %>
  date_text: <%= pub.date_text %>
  title: |
    <%= pub.title %>
  country: <%= pub.country %>
  journal: |
    <%= pub.journal %>
  created_at: <%= pub.created_at %>
  updated_at: <%= pub.updated_at %>
<% end %>