<% for proposal in @proposals %>
<%= proposal.id %>:
  id: <%= proposal.id %>
  date_submitted: <%= proposal.date_submitted %>
  date_agm: <%= proposal.date_agm %>
  company: |
    <%= proposal.company %>
  company_url: |
    <%= proposal.company_url %>
  ticker: <%= proposal.ticker %>
  proposal_type: |
    <%= proposal.proposal_type %>
  proposal_url: |
    <%= proposal.proposal_url %>
  correspondence: |
    <% if proposal.correspondence %><%= proposal.correspondence.gsub(/\n/,"\n    ") %><% end %>
  proxy_url: |
    <%= proposal.proxy_url %>
  in_proxy: |
    <% if proposal.in_proxy %><%= 1 %><% end %>
  yes_percent: <%= proposal.yes_percent %>
  created_at: <%= proposal.created_at %>
  updated_at: <%= proposal.updated_at %>
<% end %>