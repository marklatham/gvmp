<h1>Listing proposals</h1>

<table>
  <tr>
    <th>Date submitted</th>
    <th>Date agm</th>
    <th>Company</th>
    <th>Company url</th>
    <th>Ticker</th>
    <th>Proposal type</th>
    <th>Proposal url</th>
    <th>Correspondence</th>
    <th>Result</th>
  </tr>

<% @proposals.each do |proposal| %>
  <tr>
    <td><%=h proposal.date_submitted %></td>
    <td><%=h proposal.date_agm %></td>
    <td><%=h proposal.company %></td>
    <td><%=h proposal.company_url %></td>
    <td><%=h proposal.ticker %></td>
    <td><%=h proposal.proposal_type %></td>
    <td><%=h proposal.proposal_url %></td>
    <td><%=h proposal.correspondence %></td>
    <td><%=h proposal.result %></td>
    <td><%= link_to 'Show', proposal %></td>
    <td><%= link_to 'Edit', edit_proposal_path(proposal) %></td>
    <td><%= link_to 'Destroy', proposal, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New proposal', new_proposal_path %>