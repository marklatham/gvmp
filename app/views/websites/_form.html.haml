- form_for(@website) do |f|
  = f.error_messages

  %p
    = f.label :title
    %br
    = f.text_field :title, :size => 70
  
  %p
    = f.label :url
    %br
    = f.text_field :url, :size => 70
  
  %p
    = f.label :feed_url
    %br
    = f.text_field :feed_url, :size => 70
  
  %p
    = f.submit "Submit"

%br
%br

