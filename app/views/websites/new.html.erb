<h2>Add a website/blog to <%=h @community.short_name %> community ranking</h2>

<% form_for @website, :url => { :action => :create, :community => @community} do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit "Add" %>
  </p>
<% end %>

<br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
