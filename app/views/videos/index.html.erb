<h1>Listing videos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Tags</th>
    <th>Category</th>
    <th>Url</th>
    <th>Transcript</th>
  </tr>

<% @videos.each do |video| %>
  <tr>
    <td><%=h video.title %></td>
    <td><%=h video.description %></td>
    <td><%=h video.tags %></td>
    <td><%=h video.category %></td>
    <td><%=h video.url %></td>
    <td><%=h video.transcript %></td>
    <td><%= link_to 'Show', video %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New video', new_video_path %>