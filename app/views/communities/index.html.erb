<h1>Listing communities</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Short name</th>
    <th>Category</th>
    <th>Country</th>
    <th>Prov state</th>
    <th>City</th>
    <th>Official url</th>
    <th>Wiki url</th>
    <th>Description</th>
  </tr>

<% for community in @communities %>
  <tr>
    <td><%=h community.name %></td>
    <td><%=h community.short_name %></td>
    <td><%=h community.category %></td>
    <td><%=h community.country %></td>
    <td><%=h community.prov_state %></td>
    <td><%=h community.city %></td>
    <td><%=h community.official_url %></td>
    <td><%=h community.wiki_url %></td>
    <td><%=h community.description %></td>
    <td><%= link_to 'Show', community %></td>
    <td><%= link_to 'Edit', edit_community_path(community) %></td>
    <td><%= link_to 'Destroy', community, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New community', new_community_path %>
