<% for community in @communities %>
<%= community.id %>:
  id: <%= community.id %>
  name: |
    <%= community.name %>
  short_name: |
    <%= community.short_name %>
  category: <%= community.category %>
  country: <%= community.country %>
  prov_state: <%= community.prov_state %>
  city: <%= community.city %>
  scope: <%= community.scope %>
  wiki_url: <%= community.wiki_url %>
  official_url: <%= community.official_url %>
  description: |
    <% if community.description %><%= community.description.gsub(/\n/,"\n    ") %><% end %>
  created_at: <%= community.created_at %>
  updated_at: <%= community.updated_at %>
  add_to_description: |
    <% if community.add_to_description %><%= community.add_to_description.gsub(/\n/,"\n    ") %><% end %>
  creator_ip: <%= community.creator_ip %>
  adder_ip: <%= community.adder_ip %>
  delta: |
    <% if community.delta %><%= 1 %><% end %>
<% end %>
