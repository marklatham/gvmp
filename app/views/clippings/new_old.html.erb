<h1>New clipping</h1>

<% form_for(@clipping) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :date_sequence %><br />
    <%= f.datetime_select :date_sequence %>
  </p>
  <p>
    <%= f.label :date_text %><br />
    <%= f.text_field :date_text %>
  </p>
  <p>
    <%= f.label :source %><br />
    <%= f.text_field :source %>
  </p>
  <p>
    <%= f.label :source_url %><br />
    <%= f.text_field :source_url %>
  </p>
  <p>
    <%= f.label :author %><br />
    <%= f.text_field :author %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :title_url %><br />
    <%= f.text_field :title_url %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', clippings_path %>